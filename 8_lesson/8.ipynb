    "def f(a, p):\n",
    "    s = ''\n",
    "    while a > 0:\n",
    "        s = s + str(a%p)\n",
    "        a = a//p\n",
    "    s = s[::-1]\n",
    "    return s\n",
    "\n",
    "\n",
    "c = f(194, 6)\n",
    "print(c)\n",
